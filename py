friend_list = ['Andre','Angela','Tyogwen','Gaby']
print(friend_list) 

friend_list = ['Andre','Angela','Tyogwen','Gaby']
friend_list.append(shelly)
print(friend_list)

friend_list = ['Andre','Angela','Tyogwen','Gaby','shelly']
del friend_list[3]
print(friend_list)

friend.list = ['Andre','Angela','Tyogwen','Gaby']
friend_list.sort(reverse=true)
print(friend_list) 


